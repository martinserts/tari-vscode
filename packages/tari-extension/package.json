{
  "name": "tari-extension",
  "publisher": "tari-labs",
  "version": "0.0.1",
  "displayName": "Tari Ootle",
  "description": "Improves the Tari experience",
  "keywords": [
    "tari",
    "ootle",
    "crypto"
  ],
  "icon": "TODO",
  "main": "./dist/extension.js",
  "scripts": {
    "vscode:prepublish": "pnpm run build",
    "build": "tsc",
    "watch": "tsc --watch",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint .",
    "format": "prettier --check ."
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@eslint/js": "catalog:",
    "@types/node": "catalog:",
    "@types/vscode": "catalog:",
    "eslint": "catalog:",
    "prettier": "catalog:",
    "typescript": "catalog:",
    "typescript-eslint": "catalog:"
  },
  "dependencies": {
    "tari-extension-common": "workspace:^"
  },
  "activationEvents": [],
  "contributes": {
    "commands": [
      {
        "command": "extension.tari",
        "title": "Tari Extension"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "tariActivityBarContainer",
          "title": "Tari",
          "icon": "resources/tari.svg"
        }
      ]
    },
    "views": {
      "tariActivityBarContainer": [
        {
          "type": "webview",
          "id": "tariActivityBarView",
          "name": "Tari"
        }
      ]
    }
  },
  "engines": {
    "vscode": "^1.74.0"
  },
  "packageManager": "pnpm@10.4.1"
}
